{
  "name": "git8",
  "version": "0.0.1",
  "description": "GitHub notification tool",
  "productName": "GIT8",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "Dmitry Kryukov <remasik@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m electron",
    "build": "quasar build -m electron",
    "publish": "quasar build -m electron -P always",
    "build-linux": "quasar build -m electron --target linux",
    "publish-linux": "quasar build -m electron --target linux -P always",
    "build-windows": "quasar build -m electron --target win",
    "publish-windows": "quasar build -m electron --target windows -P always",
    "build-all": "quasar build -m electron --target all",
    "publish-all": "quasar build -m electron --target all -P always",
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@quasar/extras": "^1.4.0",
    "auto-launch": "^5.0.5",
