    "electron-store": "^5.1.0",
    "electron-unhandled": "^3.0.2",
    "electron-updater": "^4.2.0",
    "electron-util": "^0.13.1",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "quasar": "^1.7.0",
    "vue-i18n": "^8.15.3"
  },
  "devDependencies": {
    "@quasar/app": "^1.4.5",
